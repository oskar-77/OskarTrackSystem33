modules = ["python-3.11"]
[agent]
expertMode = true
integrations = ["python_database:1.0.0", "python_openai:1.0.0"]

[nix]
channel = "stable-25_05"
packages = ["freetype", "lcms2", "libimagequant", "libjpeg", "libtiff", "libwebp", "libxcrypt", "openjpeg", "pkg-config", "tcl", "tk", "xsimd", "zlib"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "OskarTrack Server"

[[workflows.workflow]]
name = "OskarTrack Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn api.main:app --host 0.0.0.0 --port 5000 --reload"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80
